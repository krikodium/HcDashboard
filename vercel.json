{
  "functions": {
    "backend/app.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/backend/app.py" },
    { "src": "/(.*)", "dest": "/frontend/build/$1" }
  ],
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ]
}